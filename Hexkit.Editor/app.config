<?xml version="1.0" standalone="yes"?>
<configuration>
  <appSettings>
    <add key="Application" value="Editor"/>
  </appSettings>
  <runtime>
    <assemblyBinding>
      <publisherPolicy apply="yes"/>
    </assemblyBinding>
    <gcConcurrent enabled="true"/>
  </runtime>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/>
  </startup>
  <system.diagnostics>
    <trace autoflush="true" indentsize="2">
      <listeners>
        <add name="Hexkit.Editor.Listener" type="System.Diagnostics.TextWriterTraceListener" initializeData="Hexkit.Editor.log"/>
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>
